{
    "name": "not-error-reporter",
    "version": "0.0.1",
    "description": "not-* family error reporter in not- environment",
    "main": "index.js",
    "scripts": {
        "test": "./node_modules/.bin/mocha --reporter spec",
        "pretest": "./node_modules/.bin/eslint ./src/**/**",
        "lint": "./node_modules/.bin/eslint ./src/**/**",
        "docs": "./node_modules/.bin/jsdoc -c jsdoc.json",
        "cover": "istanbul cover _mocha $(find ./test -name \"*.js\" -not -path \"./node_modules/*\")"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/interrupter/not-error-reporter.git"
    },
    "keywords": [
        "not-framework",
        "not-node",
        "error",
        "reporter"
    ],
    "author": "interrupter <alex.repin@mail.ru> (http://github.com/interrupter)",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/interrupter/not-error-reporter/issues"
    },
    "homepage": "https://github.com/interrupter/not-error-reporter#readme",
    "dependencies": {
        "not-error": "*",
        "not-config": "*"
    },
    "devDependencies": {
        "chai": "*",
        "chai-as-promised": "*",
        "eslint": "*",
        "ink-docstrap": "^1.3.2",
        "istanbul": "*",
        "jsdoc": "^3.5.5",
        "mocha": "*"
    }
}
